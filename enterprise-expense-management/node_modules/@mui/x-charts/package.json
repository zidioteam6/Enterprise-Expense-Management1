  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mui/mui-x/issues"
  },
  "homepage": "https://mui.com/x/react-charts/",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "keywords": [
    "react",
    "react-component",
    "mui",
    "mui-x",
    "material-ui",
    "material design",
    "charts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/mui-x.git",
    "directory": "packages/x-charts"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.1",
    "@mui/utils": "^7.0.2",
    "bezier-easing": "^2.1.0",
    "clsx": "^2.1.1",
    "prop-types": "^15.8.1",
    "reselect": "^5.1.1",
    "use-sync-external-store": "^1.5.0",
    "@mui/x-charts-vendor": "8.4.0",
    "@mui/x-internals": "8.5.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.15.14 || ^6.0.0 || ^7.0.0",
    "@mui/system": "^5.15.14 || ^6.0.0 || ^7.0.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@emotion/react": {
      "optional": true
    },
    "@emotion/styled": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "private": false,
  "module": "./esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./*/index.d.ts",
        "default": "./*/index.js"
      },
      "import": {
        "types": "./esm/*/index.d.ts",
        "default": "./esm/*/index.js"
      }
    },
    "./esm": null,
    "./modern": null
  },
  "types": "./index.d.ts"
}